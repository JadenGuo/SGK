
input, select {
    font-size:0.9em;
}

table {
    margin-top:-10px;
    margin-left:7px;
}

h4 {
    font-size:1.1em;
    text-decoration:none;
    font-weight:normal;
    color:#23A4FF;
}

a {
    color:#23A4FF;
}

.small_font_size{
    -webkit-transform: scale(0.9,0.9);
}
#news_log_records li{
	text-overflow:ellipsis; 
	overflow:hidden; padding:5px;
	-o-text-overflow:ellipsis;
	white-space:nowrap;
}

<!-- 设置滚动公告的样式  li ul-->
#scrollDiv ul,li{margin:0;padding:0}
#scrollDiv{

background-color: #FFFFFF;
width:300px;
height:280px;
line-height:25px;
margin-left:2px;
overflow:hidden
}
#scrollDiv li{height:25px;padding-left:10px;text-overflow:ellipsis; overflow:hidden;-o-text-overflow:ellipsis;white-space:nowrap;}


<!-- 设置滚动公告的样式  li ul-->
#scrollDiv2 ul,li{margin:0;padding:0}
#scrollDiv2{

background-color: #FFFFFF;
width:300px;
height:280px;
line-height:25px;
margin-left:2px;
overflow:hidden
}
#scrollDiv2 li{height:25px;padding-left:10px;text-overflow:ellipsis; overflow:hidden;-o-text-overflow:ellipsis;white-space:nowrap;}


#line-container-pu-number {
       height:160px;
       min-width: 390px;
       max-width: 420px;
}
#container-monitor-1,#container-monitor-3,#container-monitor-4 {
       height:150px;
       min-width: 390px;
       max-width: 420px;

}

#pm_number,#vm_number{
font-size: 30px;
color: #017CB9;
}

#container-monitor-2{
       height:165px;
       min-width: 390px;
       max-width: 420px;

}

#id-list {
background-color: #EEEEEE;
border: 1px solid #CCCCCC;
width:260px;
margin: 2px 50px 2px 60px;
padding: 5px;
text-align: left;
text-indent: 2px;
}


.cener-container-show{
font-size: 18px;
font-weight: bold;
color: #000;
}

.query-color {
border: 1px solid #999999;
float: left;
height: 10px;
margin: 3px 3px 0 0;
width: 10px;
}



fieldset {

width:400px;
margin-top:5px;

background-color: #fff;
border: 1px solid #ddd;
padding: 4px 5px;

-webkit-box-shadow: 3px 3px 4px #dddddd;
box-shadow: 3px 3px 4px #dddddd;
width: 95%;
margin: 0 auto;
margin-top: 5px;
margin-bottom: 20px;
padding-bottom:4px;

}
#platform_log_records{

padding-bottom:10px;

}


legend{
font-size: 16px;
font-weight: bold;
color: #000;

}


#container {

    width: 100%;
    height: 650px;
    margin:0 auto;
    position:relative;
}

#left-container, 
#right-container, 
#center-container {
    height:900px;
    position:absolute;
    top:0;
}

#left-container, #right-container {
    width:330px;
    text-align: left;
    border-bottom:1px solid #ddd;
}

#left-container {
    left:5px;
    background-position:center right;
    
}

#right-container {
    right:0;
    background-position:center left;
    border-right:1px solid #ddd;
}

#right-container h4{
    text-indent:8px;
}

#center-container {
    width:800px;
    left:520px;
    color:#ccc;
}

.text {
    margin: 7px;
}

#inner-details {
    font-size:0.8em;
    list-style:none;
    margin:7px;
}

#log {
    position:absolute;
    top:10px;
    font-size:1.0em;
    font-weight:bold;
    color:#23A4FF;
}


#infovis {
    position:relative;
    width:800px;
    height:680px;
    margin:auto;
    overflow:block;
}

/*TOOLTIPS*/
.tip {
    color: #111;
    width: 139px;
    background-color: white;
    border:1px solid #ccc;
    -moz-box-shadow:#555 2px 2px 8px;
    -webkit-box-shadow:#555 2px 2px 8px;
    -o-box-shadow:#555 2px 2px 8px;
    box-shadow:#555 2px 2px 8px;
    opacity:0.9;
    filter:alpha(opacity=90);
    font-size:10px;
    font-family:Verdana, Geneva, Arial, Helvetica, sans-serif;
    padding:7px;
}

